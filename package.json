	"scripts": {
		"start": "theme open && npm run build && concurrently --kill-others \"npm run watch:src\" \"npm run watch:theme\"",
		"start:win": "npm run build && concurrently --kill-others \"npm run watch:src\" \"npm run watch:theme\"",
		"watch:src": "gulp watch",
		"watch:theme": "theme watch",
		"build": "gulp",
		"build:prod": "gulp --production",
		"release": "npm run build && gulp release",
		"release:prod": "npm run build:prod && gulp release",
		"deploy": "npm run build && theme deploy",
		"deploy:prod": "npm run build:prod && theme deploy"
	},
